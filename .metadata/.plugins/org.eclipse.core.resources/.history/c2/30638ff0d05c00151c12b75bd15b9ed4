var win = Ti.UI.createWindow({
	backgroundColor: "white",
	layout: "vertical"
});

//located within a geo.js module, perhaps?
var runGeo = function(){
	Ti.Geolocation.purpose = "Your location is needed to gather lat/long coords."; 
	Ti.Geolocation.getCurrentPosition(function(e){
                console.log(e);  //good idea to console log to see what's available to you
		var lat = e.coords.latitude;
		var lng = e.coords.longitude;
                var ui = require("ui");
		ui.buildUi(lat, lng); 
	});
}; //runGeo closing 



win.add(titleView, locName);
win.open();
