var win = Ti.UI.createWindow({
	backgroundColor: "#fff",
	layout: "vertical"
});

var btn2 = Ti.UI.createView({
	top: 100,
	width: "100%",
	height: 50,
	backgroundColor: "#9f9f9f"
});
var btn2txt = Ti.UI.createLabel({
	text: "Make Memories",
	left: 10,
	color: "#fff",
	font: {fontSize: 35}
});
btn2.add(btn2txt);


var btn1 = Ti.UI.createView({
	top: 100,
	width: "100%",
	height: 50,
	backgroundColor: "#9f9f9f"
});
var btn1txt = Ti.UI.createLabel({
	text: "Make Memories",
	left: 10,
	color: "#fff",
	font: {fontSize: 35}
});
btn1.add(btn1txt);



win.add(btn1);
win.add(btn2);
win.open();
