var getData=function(){var e=require("geo"),o="https://api.foursquare.com/v2/venues/search?query=photo,studio&limit=10&radius=10000&ll="+e.loc+"&oauth_token=JQIRJBG4OGIU45V4XUJGKYCBJDPWU4DAVV3JSIFUXL4DBY5I&v=20151019";if(Ti.Network.online){var s=Ti.Network.createHTTPClient();s.onload=function(e){var o=JSON.parse(this.responseText),s=[];for(i=0,j=o.response.venues.length;i<j;i++){var t={name:o.response.venues[i].name,phone:o.response.venues[i].contact.formattedPhone,street:o.response.venues[i].location.address,city:o.response.venues[i].location.city,state:o.response.venues[i].location.state,zip:o.response.venues[i].location.postalCode,crossStreet:o.response.venues[i].location.crossStreet};s.push(t)}var r=require("db");r.save(t);var n=require("ui");n.buildUI(s);var a=require("cloud");a.login(s)},s.open("GET",o),s.send()}else alert("Network currently unavailable.")};exports.getData=getData;