var win=Ti.UI.createWindow({backgroundColor:"#fff",layout:"vertical"}),btn3=Ti.UI.createView({top:50,width:"100%",height:50,backgroundColor:"#9f9f9f"}),btn3txt=Ti.UI.createLabel({text:"Take A Picture",color:"#fff",font:{fontSize:35}});btn3.add(btn3txt),btn3.addEventListener("click",function(){Titanium.Media.showCamera({success:function(e){Ti.API.debug("Our type was: "+e.mediaType),e.mediaType==Ti.Media.MEDIA_TYPE_PHOTO||alert("got the wrong type back ="+e.mediaType)},cancel:function(){},error:function(e){var t=Titanium.UI.createAlertDialog({title:"Camera"});e.code==Titanium.Media.NO_CAMERA?t.setMessage("Please run this test on device"):t.setMessage("Unexpected error: "+e.code),t.show()},saveToPhotoGallery:!0,allowEditing:!0,mediaTypes:[Ti.Media.MEDIA_TYPE_VIDEO,Ti.Media.MEDIA_TYPE_PHOTO]})});var nearMe=Ti.UI.createLabel({top:15,text:"Photo Studios Near Me",font:{fontSize:35,fontWeight:"bold"}}),list=Ti.UI.createScrollView({top:15,bottom:0,width:"100%",height:820,showVerticalScrollIndicator:!0,backgroundColor:"#fff",layout:"vertical"}),buildUI=function(e){for(i=0,j=e.length;i<j;i++){var t=Ti.UI.createView({width:"80%",backgroundColor:"#fff",height:200,layout:"vertical"});list.add(t);var o=Ti.UI.createLabel({top:5,font:{fontSize:20,fontWeight:"bold"},text:e[i].name}),a=Ti.UI.createLabel({font:{fontSize:20},text:e[i].phone}),r=Ti.UI.createLabel({font:{fontSize:20},text:e[i].street+", "+e[i].city+", "+e[i].state+" "+e[i].zip}),n=Ti.UI.createLabel({font:{fontSize:20},text:e[i].crossStreet});t.add(o),t.add(a),t.add(r),t.add(n)}};exports.buildUI=buildUI,win.add(btn3),win.add(nearMe),win.add(list),win.open();