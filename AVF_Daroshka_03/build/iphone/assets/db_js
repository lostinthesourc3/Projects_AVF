var read=function(){var e=Ti.Database.open("stockDB");e.execute("CREATE TABLE IF NOT EXISTS stockTBL (id INTEGER PRIMARY KEY, name TEXT, phone TEXT, street TEXT, city TEXT, state TEXT, zip TEXT, crossStreet TEXT)");for(var i=e.execute("SELECT * FROM stockTBL");i.isValidRow();){({name:i.fieldByName("name"),phone:i.fieldByName("phone"),street:i.fieldByName("address"),city:i.fieldByName("city"),state:i.fieldByName("state"),zip:i.fieldByName("zip"),crossStreet:i.fieldByName("crossStreet")});i.next()}i.close(),e.close()},save=function(e){var t=Ti.Database.open("stockDB");for(t.execute("CREATE TABLE IF NOT EXISTS stockTBL (id INTEGER PRIMARY KEY, name TEXT, phone TEXT, street TEXT, city TEXT, state TEXT, zip TEXT, crossStreet TEXT)"),t.execute("DELETE FROM stockTBL"),i=0,j=e.length;i<j;i++)t.execute("INSERT INTO stockTBL (name, phone, street, city, state, zip, crossStreet) VALUES (?, ?, ?, ?, ?, ?, ?)",e[i].name,e[i].phone,e[i].street,e[i].city,e[i].state,e[i].zip,e[i].crossStreet);console.log(e),t.close(),read()};exports.save=save;