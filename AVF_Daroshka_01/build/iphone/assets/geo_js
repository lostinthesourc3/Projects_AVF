var getGeo=function(){Ti.Geolocation.locationServicesEnabled&&(Ti.Geolocation.purpose="Get current location",Ti.Geolocation.accuracy=Ti.Geolocation.ACCURACY_BEST,Ti.Geolocation.distaceFilter=10,Titanium.Geolocation.getCurrentPosition(function(e){if(e.error)alert("Enable location services"+e.error);else{Ti.API.info(e.coords),console.log(e.coords);var o=e.coords.latitude,i=e.coords.longitude,r="http://api.wunderground.com/api/d1e578e5aa05dc1d/conditions/q/"+o+","+i+".json";if(Ti.Network.online){var t=Ti.Network.createHTTPClient();t.onload=function(e){console.log(e);var o=JSON.parse(this.responseText),i={loc:o.current_observation.display_location.full,temp:o.current_observation.temp_f+"ÂºF",feelLike:o.current_observation.feelslike_f,weather:o.current_observation.weather,humid:o.current_observation.relative_humidity},r=require("ui");console.log(i),r.buildUI(i)},t.open("GET",r),t.send()}else alert("Network currently unavailable.")}}))};exports.getGeo=getGeo;