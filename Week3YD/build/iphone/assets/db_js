var read=function(){for(var e=Ti.Database.open("stockDB"),a=e.execute("SELECT * FROM stockTBL"),i=[];a.isValidRow();){var r={username:a.fieldByName("username"),image:a.fieldByName("image"),text:a.fieldByName("text")};i.push(r),a.next()}a.close(),e.close(),console.log(r);var s=require("ui");s.buildUI(i)},save=function(e){var a=Ti.Database.open("stockDB");for(a.execute("CREATE TABLE IF NOT EXISTS stockTBL (id INTEGER PRIMARY KEY, username TEXT, image TEXT, text TEXT)"),a.execute("DELETE FROM stockTBL"),i=0,j=e.length;i<j;i++)a.execute("INSERT INTO stockTBL (username, image, text) VALUES (?, ?, ?)",e[i].username,e[i].image,e[i].text);console.log(e),a.close(),read()};exports.save=save;