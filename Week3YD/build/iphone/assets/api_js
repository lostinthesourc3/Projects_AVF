var getData=function(){var a="https://api.instagram.com/v1/tags/catstagram/media/recent?client_id=c2b82a912a31406d90e4fa60480ba01b";if(Ti.Network.online){var e=Ti.Network.createHTTPClient();e.onload=function(a){console.log(a);var e=JSON.parse(this.responseText),t=[];for(i=0,j=e.data.length;i<j;i++){var r={username:e.data[i].caption.from.username,image:e.data[i].images.standard_resolution.url,text:e.data[i].caption.text};t.push(r)}var n=require("db");n.save(t);var o=require("cloud");o.save(t);var s=require("ui");s.buildUI(t)},e.open("GET",a),e.send()}else alert("Network currently unavailable.")};exports.getData=getData;