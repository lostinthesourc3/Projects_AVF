var Cloud=require("ti.cloud");Cloud.debug=!0;var loginUser=function(){Cloud.Users.login({login:"admin",password:"1234"},function(e){if(e.success){var a=e.users[0];Ti.API.info("Success!\nACS User ID: "+a.id+"\nACS App sessionId: "+Cloud.sessionId+"\nACS App Username: "+a.username)}else alert(e.error&&e.message||JSON.stringify(e))})};loginUser();var save=function(e){for(i=0,j=e.length;i<j;i++)Cloud.Objects.create({classname:"insta",fields:{username:e[i].username,image:e[i].image,text:e[i].text}},function(e){e.success?console.log("SAVED TO CLOUD"):console.log("DATA NOT SAVED")})};exports.loginUser=loginUser,exports.save=save;