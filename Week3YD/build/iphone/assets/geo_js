var getGeo=function(){if("android"===Ti.Platform.osname){var e=40.88010787963867,a=-74.11193084716797,o=e+","+a;exports.loc=o;var r=require("api");r.getData(e,a)}else Titanium.Geolocation.purpose="Get Current Position",Titanium.Geolocation.getCurrentPosition(function(e){if(console.log(e),e.error)alert("Enable location services");else{Ti.API.info(e.coords),console.log(e.coords);var a=e.coords.latitude,o=e.coords.longitude,r=a+","+o;exports.loc=r;var i=require("api");i.getData(a,o)}})};exports.getGeo=getGeo;