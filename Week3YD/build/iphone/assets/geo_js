var getGeo=function(){if("android"===Ti.Platform.osname){var e=40.88010787963867,a=-74.11193084716797,i=e+","+a;exports.loc=i;var o=require("api");o.getData(e,a)}else Titanium.Geolocation.purpose="Get Current Position",Titanium.Geolocation.getCurrentPosition(function(e){if(console.log(e),e.error)alert("Enable location services");else{Ti.API.info(e.coords),console.log(e.coords);var a=e.coords.latitude,i=e.coords.longitude,o=a+","+i;exports.loc=o;var r=require("api");r.getData(a,i)}})};exports.getGeo=getGeo;